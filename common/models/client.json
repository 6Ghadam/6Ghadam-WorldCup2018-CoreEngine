{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "realm",
    "emailVerified"
  ],
  "properties": {
    "password": {
      "type": "string",
      "required": true,
      "index": true,
      "min": 6
    },
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "time": {
      "type": "number",
      "required": true,
      "index": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "referrer": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "accountInfo": {
      "type": "embedsOne",
      "model": "accountInfo",
      "property": "accountInfoModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "referrals": {
      "type": "embedsOne",
      "model": "referral",
      "property": "referralModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "trophy": {
      "type": "embedsOne",
      "model": "trophy",
      "property": "trophyModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "checkpoint": {
      "type": "embedsOne",
      "model": "checkpoint",
      "property": "checkpointModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "transactions": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": "clientId"
    },
    "estimates": {
      "type": "hasMany",
      "model": "estimate",
      "foreignKey": "clientId"
    },
    "choices": {
      "type": "hasMany",
      "model": "choice",
      "foreignKey": "clientId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changePassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "sendPassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "updateById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__accountInfo"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__transactions"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__estimates"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__choices"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "nextObject"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "statistics"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "changePhone"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
